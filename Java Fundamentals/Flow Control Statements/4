import java.util.Scanner;
public class tm_01_handson_7 {

	public static void main(String[] args) {
		Scanner a = new Scanner(System.in);
		Scanner b = new Scanner(System.in);
		char it = a.next().charAt(0);
		char ti = b.next().charAt(0);
		if (it < ti) {
			System.out.println(it+","+ti);
		}
		else {
			System.out.println(ti+ "," +it);
		}

	}

}
